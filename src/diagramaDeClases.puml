class LineaDeTransporte{
    String nombre;
    Estacion origen
    Estacion destino
    List<Estacion> estaciones
    MedioDeTransporte medioTransporte
}
Enum MedioDeTransporte{
    SUBTERRANEO
    FERROCARRIL
}

class Servicio{
    String descripcion
    List<Servicio> subServicios
}

class MedioElevacion{
    PuntoElevacion origen
    PuntoElevacion destino
}

Enum PuntoElevacion {
    CALLE
    ANDEN
    BARRERA
}

class Estacion{
    String nombre
    Ubicacion ubicacion
    List<Servicio> servicios
}

class Ubicacion{
    int latitud
    int longitud
}

class Comunidad{
    List <Miembro> miembros
    List <Usuario> administradores
}
class Miembro{
    String nombre
    String apellido
